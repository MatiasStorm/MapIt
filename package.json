{
  "name": "map-it",
  "version": "1.0.0",
  "description": "NodeJS 2021 exam project",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ./src/app.js",
    "lint": "eslint ./src ./db",
    "lint-fix": "eslint ./src ./db --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate-undo": "sequelize-cli db:migrate:undo",
    "add-migration": "sequelize-cli migration:generate --name ",
    "build-css": "tailwindcss -i ./src/public/css/main.css -o ./src/public/css/tailwind.css",
    "watch-css": "tailwindcss -o ./src/public/css/tailwind.css --watch"
  },
  "author": "Matias Storm",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.12.0-alpha.1",
    "tailwindcss": "^3.0.0",
    "socket.io": "^4.4.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0"
  }
}
